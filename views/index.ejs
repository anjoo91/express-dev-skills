<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Express Dev Skills</title>
</head>
<body>
    <h1>Express Dev Skills</h1>
    <a href="/skills/new">Add Skill</a> <!-- New link to add a skill -->
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
          <a href="/skills/<%= s.id %>"><%= s.name %></a>
        </li>
      <% }); %>
    </ul>
</body>
</html>
